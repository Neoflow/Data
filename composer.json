{
  "name": "neoflow/data",
  "description": "Data handler for arrays and ArrayAccess-implementations.",
  "license": "MIT",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Jonathan Nessier",
      "homepage": "https://www.neoflow.ch"
    }
  ],
  "autoload": {
    "psr-4": {
      "Neoflow\\Data\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Neoflow\\Data\\Test\\": "tests/"
    }
  },
  "scripts": {
    "run php-cs-fixer": "php-cs-fixer fix",
    "run php-cs-fixer dry": "php-cs-fixer fix --dry-run",
    "run phpunit": "phpunit --configuration phpunit.xml.dist"
  },
  "require": {
    "php": "^7.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.2",
    "php-coveralls/php-coveralls": "^2.2",
    "pds/skeleton": "^1.0"
  }
}
